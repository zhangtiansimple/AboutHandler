<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abouthandler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HandlerForMainThreadActivity"/>
        <activity android:name=".SubThreadUpdateUIActivity"/>
        <activity android:name=".HandlerForSubThreadActivity"/>
        <activity android:name=".HandlerThreadActivity"/>
        <activity android:name=".CustomHandlerActivity"/>
        <activity android:name=".myhandler.MyHandlerActivity"/>
        <activity android:name=".HandlerNormalQuestionActivity"/>
    </application>

</manifest>